@model List<PersonInfo>

<h2>Actor List</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Id</td>
            <td>@p.Name</td>
            <td>@p.Age</td>
            <td>@p.Skills</td>

        </tr>
    }
</table>
